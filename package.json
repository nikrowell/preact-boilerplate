{
  "name": "preact-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "start": "rm -fr .cache/ && parcel index.html --open",
    "build": "rm -rf dist/* && parcel build index.html && npm run size",
    "size": "echo $(gzip -c dist/main.*.js | wc -c)"
  },
  "dependencies": {
    "glslify": "^6.3.1",
    "gsap": "^2.0.2",
    "load-asset": "^1.2.0",
    "ogl": "0.0.9",
    "preact": "^8.3.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-preact": "^1.1.0",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7"
  }
}
